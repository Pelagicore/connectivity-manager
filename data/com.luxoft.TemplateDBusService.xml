<!DOCTYPE node PUBLIC
 "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
 "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.luxoft.TemplateDBusService">
    <method name="RemoveMeFoo">
      <arg name="number" type="u" direction="in"/>
      <arg name="is_odd" type="b" direction="out"/>
    </method>

    <signal name="RemoveMeBar">
      <arg name="something" type="u"/>
    </signal>

    <property name="RemoveMeBaz" type="b" access="readwrite"/>
  </interface>
</node>
